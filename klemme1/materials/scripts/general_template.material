import * from "grid.material"

vertex_program Template/DepthMapVS glsl
{
  source depth_map.vert

  default_params
  {
    param_named_auto texelOffsets texel_offsets
  }
}

fragment_program Template/DepthMapFS glsl
{
  source depth_map.frag

  default_params
  {
    param_named_auto pNear near_clip_distance
    param_named_auto pFar far_clip_distance
  }
}

material Template/DepthMap
{
  technique
  {
    pass
    {
      vertex_program_ref Template/DepthMapVS { }
      fragment_program_ref Template/DepthMapFS { }
    }
  }
}

vertex_program Template/XYZPointsVS glsl
{
  source depth_points_map.vert
}

fragment_program Template/XYZPointsFS glsl
{
  source depth_points_map.frag

  default_params
  {
    param_named_auto width viewport_width
    param_named_auto height viewport_height
  }
}

material Template/XYZPoints
{
  technique
  {
    pass pcd_tex
    {
      separate_scene_blend one zero one zero

      vertex_program_ref Template/XYZPointsVS { }
      fragment_program_ref Template/XYZPointsFS { }
    }
  }
}

vertex_program Template/LaserScan1stVS glsl
{
  source laser_1st_pass.vert
}

fragment_program Template/LaserScan1stFS glsl
{
  source laser_1st_pass.frag

  default_params
  {
    param_named retro float 0.0
    param_named_auto near near_clip_distance
    param_named_auto far far_clip_distance
  }
}

material Template/LaserScan1st
{
  technique
  {
    pass laser_tex
    {
      separate_scene_blend one zero one zero

      vertex_program_ref Template/LaserScan1stVS { }
      fragment_program_ref Template/LaserScan1stFS { }
    }
  }
}

vertex_program Template/LaserScan2ndVS glsl
{
  source laser_2nd_pass.vert
}

fragment_program Template/LaserScan2ndFS glsl
{
  source laser_2nd_pass.frag

  default_params
  {
    param_named tex1 int 0
    param_named tex2 int 1
    param_named tex3 int 2
    param_named_auto texSize texture_size 0
  }
}

material Template/LaserScan2nd
{
  technique
  {
    pass laser_tex_2nd
    {
      separate_scene_blend one zero one zero

      vertex_program_ref Template/LaserScan2ndVS { }
      fragment_program_ref Template/LaserScan2ndFS { }
    }
  }
}

